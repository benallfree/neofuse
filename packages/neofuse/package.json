{
  "name": "neofuse",
  "version": "0.0.1",
  "description": "FUSE bindings for Node",
  "main": "index.js",
  "bin": {
    "neofuse": "./bin.js"
  },
  "scripts": {
    "install": "node-gyp-build",
    "test": "tape test/*.js",
    "prebuild": "prebuildify --napi --strip",
    "prebuild-ia32": "prebuildify --napi --strip --arch=ia32",
    "configure": "NODE=$(which node) && sudo -E $NODE ./bin.js configure || true"
  },
  "gypfile": true,
  "dependencies": {
    "neofuse-shared-library": "workspace:*",
    "nanoresource": "^1.3.0",
    "napi-macros": "^2.0.0",
    "node-gyp-build": "^4.2.0"
  },
  "devDependencies": {
    "concat-stream": "^2.0.0",
    "prebuildify": "^3.0.4",
    "standard": "^13.1.0",
    "tape": "^4.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benallfree/neofuse.git"
  },
  "author": {
    "name": "Ben Allfree",
    "url": "https://x.com/benallfree"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benallfree/neofuse/issues"
  },
  "homepage": "https://github.com/benallfree/neofuse"
}
